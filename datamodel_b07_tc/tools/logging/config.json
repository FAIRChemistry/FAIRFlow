{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s - %(lineno)d",
            "datefmt": "%m-%d-%y %H:%M:%S"
        }
    },
    "handlers": {
        "file": {
            "class": "logging.FileHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "filename": "datamodel_b07_tc/tools/logging/log_faraday.log",
            "mode": "a"
        },
        "console": {
            "class": "logging.StreamHandler",
            "level": "WARNING",
            "formatter": "simple"
        }
    },
    "root": {
        "level": "DEBUG",
        "handlers": [
            "console",
            "file"
        ]
    }
}